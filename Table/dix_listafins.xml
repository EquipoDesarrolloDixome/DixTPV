<?xml version="1.0" encoding="UTF-8"?>
<table>
    <column>
        <name>idlista</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>codpago</name>
        <type>character varying(10)</type>
        <null>NO</null>
    </column>
    <column>
        <name>codigofactura</name>
        <type>character varying(25)</type>
    </column>
    <column>
        <name>idcaja</name>
        <type>integer</type>
        <null>no</null>
    </column>
    <column>
        <name>idfactura</name>
        <type>integer</type>
        <null>no</null>
    </column>
    <column>
        <name>total</name>
        <type>double precision</type>
        <default>0</default>
    </column>
    <column>
        <name>importeentregado</name>
        <type>double precision</type>
        <default>0</default>
    </column>
    <constraint>
        <name>dix_listafins_pkey</name>
        <type>PRIMARY KEY (idlista)</type>
    </constraint>
    <constraint>
        <name>dix_listafins_codpago</name>
        <type>FOREIGN KEY (codpago) REFERENCES formaspago (codpago) ON DELETE RESTRICT ON UPDATE CASCADE</type>
    </constraint> 
    <constraint>
        <name>dix_listafins_idcaja</name>
        <type>FOREIGN KEY (idcaja) REFERENCES dixtpv_cajas (idcaja) ON DELETE RESTRICT ON UPDATE CASCADE</type>
    </constraint> 
    <constraint>
        <name>dix_listafins_idfactura</name>
        <type>FOREIGN KEY (idfactura) REFERENCES facturascli (idfactura) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint> 
</table>