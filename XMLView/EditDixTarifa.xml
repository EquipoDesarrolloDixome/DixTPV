            </column>
            <column name="name" order="110" numcolumns="12">
                <widget type="text" fieldname="nombre" />
            </column>
            <column name="fecha-ini" order="120"  numcolumns="3">
                <widget type="date" fieldname="fechainitarifa" />
            </column>
            <column name="fecha-fin" order="130" numcolumns="3">
                <widget type="date" fieldname="fechainitarifa" />
            </column>
